{
  "name": "Pillard",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Captures the current frame of a YouTube video.",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
